#include<stdio.h>
int main (){
    char a;
    printf("enter the value ");
    scanf("%c",&a);
    if(a=='c'){
        printf("cat");
        }
        else{
            printf("not match");
        }
    return 0;
}


#include<stdio.h>
int main (){
    char a;
    printf("enter the character ");
    scanf("%c",&a);
    if(a=='a'){
        int a,b,c;
        printf("enter the value of a and b ");
        scanf("%d %d",&a,&b);
        c=a+b;
        printf("the sum of a and b is %d",c);
        }
        else{
            printf("not match");
        }
    return 0;
}


#include<stdio.h>
int main (){
    char a;
    printf("enter the character ");
    scanf("%c",&a);
    if(a=='s'){
        int a,b,c;
        printf("enter the value of a and b ");
        scanf("%d %d",&a,&b);
        c=a-b;
        printf("the subtraction of a and b is %d",c);
        }
        else{
            printf("not match");
        }
    return 0;
}


