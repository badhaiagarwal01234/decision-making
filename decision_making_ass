#include<stdio.h>
int main()
{
    int num;
    printf("enter the value ");
    scanf("%d",&num);
    if (num%2==0){
        printf("this is a even number ");
    }
        else{
            printf("this is a odd number ");
    }
return 0;
}
#include<stdio.h>
int main()
{
    int num;
    printf("enter the value ");
    scanf("%d",&num);
    if (num%5==0){
        printf("it is divisible by 5 ");
    }
        else{
            printf("it is not divisible by 5 ");
    }
return 0;
}


